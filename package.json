{
  "name": "rebelchat-server",
  "version": "1.0.0",
  "description": "Server support for chatback to provide (optional/configurable) chat-to-email manifests and notifications with tokens to be able to (securely) move chat sessions to other browsers",
  "main": "server/server.js",
  "engines": {
    "node": "6.x"
  },
  "scripts": {
    "start": "NODE_ENV=development node ./server/server.js",
    "debug": "NODE_ENV=development node-debug ./server/server.js",
    "test": "NODE_PATH=server/lib mocha server/test --recursive --reporter spec"
  },
  "dependencies": {
    "aguid": "^2.0.0",
    "ajv": "^4.11.5",
    "app-module-path": "^2.1.0",
    "body-parser": "^1.15.2",
    "cors": "^2.7.1",
    "dotenv-with-overload": "^0.5.1",
    "express": "^4.13.4",
    "firebase": "^3.7.3",
    "forwarded-for": "^0.1.1",
    "jsonschema": "^1.1.0",
    "jwt-simple": "^0.5.0",
    "nodemailer": "^3.1.8"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rebelstackio/rebelchat-server.git"
  },
  "author": "jegj",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rebelstackio/rebelchat-server/issues"
  },
  "homepage": "https://github.com/rebelstackio/rebelchat-server#readme"
}
